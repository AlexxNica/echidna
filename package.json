  "devDependencies": {
    "chai": "3.4",
    "chai-as-promised": "5.1",
    "chai-immutable": "1.5",
    "coveralls": "2.11",
    "istanbul": "0.4",
    "jscs": "2.7",
    "jsdoc": "3.4",
    "jshint": "2.9.1-rc1",
    "mocha": "2.3",
    "morgan": "1.6",
    "nsp": "2.1"
  },
  "scripts": {
    "check-style": "jscs .",
    "coverage": "istanbul cover _mocha",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "jsdoc": "jsdoc -a all -d doc/api/ -e utf8 -r app.js lib/ test/",
    "lint": "jshint .",
    "nsp": "nsp check",
    "start": "node app.js",
    "test": "npm run check-style && npm run lint && mocha -c -G",
    "testserver": "node test/lib/testserver"
  },
  "engines": {
    "node": ">=4.1.2",
    "npm": ">=3.3.6"
  }
